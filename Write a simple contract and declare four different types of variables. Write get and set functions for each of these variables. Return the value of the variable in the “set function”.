pragma solidity ^0.8.18;

contract SimpleContract {
    uint256 public variableA;
    bool public variableB;
    address public variableC;
    string public variableD;

    function setVariableA(uint256 _value) external {
        variableA = _value;
    }

    function getVariableA() external view returns (uint256) {
        return variableA;
    }

    function setVariableB(bool _value) external {
        variableB = _value;
    }

    function getVariableB() external view returns (bool) {
        return variableB;
    }

    function setVariableC(address _value) external {
        variableC = _value;
    }

    function getVariableC() external view returns (address) {
        return variableC;
    }

    function setVariableD(string calldata _value) external {
        variableD = _value;
    }

    function getVariableD() external view returns (string memory) {
        return variableD;
    }
    
}
